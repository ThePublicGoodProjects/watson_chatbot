{
  "name": "Chatbot",
  "description": "chatbot web integration build",
  "version": "0.1.1",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "gulp",
    "watch": "gulp watch",
    "build-prod": "gulp --prod",
    "test-integration": "casperjs test ./test/integration/test.*.js",
    "test-integration-runner": "NODE_ENV=test node casper-runner.js",
    "test": "npm run lint && npm run test-integration-runner",
    "test-unit": "jest test/unit --coverage",
    "lint": "eslint .",
    "autofix": "eslint --fix .",
    "codecov": "npm run test && (codecov || true)"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ThePublicGoodProjects/watson_chatbot.git"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dashbot": "^10.4.1",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "uuid": "^3.3.2",
    "watson-developer-cloud": "^3.18.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "casperjs": "^1.1.4",
    "codecov": "^3.5.0",
    "cryptiles": "^4.1.3",
    "eslint": "^5.16.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-concat": "^2.6.1",
    "gulp-csso": "^3.0.1",
    "gulp-eslint": "^5.0.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-uglify": "^3.0.2",
    "gulp-util": "^3.0.8",
    "jest": "^24.8.0",
    "phantomjs-prebuilt": "^2.1.16",
    "sass": "^1.22.4",
    "supertest": "^3.4.2"
  }
}
